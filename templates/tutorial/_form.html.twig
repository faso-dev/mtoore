{{ form_start(form) }}
<div class="row">
    <div class="col-lg-4">
        {{ form_row(form.title) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.category) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.url) }}
    </div>
    <div class="col-lg-12">
        {{ form_row(form.description) }}
    </div>
    <div class="col-lg-12 mt-3">
        <button class="btn btn-dark">{{ button_label|default('Enregistrer') }}</button>
    </div>
</div>
{{ form_end(form) }}
